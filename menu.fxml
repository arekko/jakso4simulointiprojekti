      <Button fx:id="driversNappi" layoutX="41.0" layoutY="364.0" mnemonicParsing="false" onAction="#driversNappiPainettu" opacity="0.0" prefHeight="84.0" prefWidth="326.0" text="Button" />
      <Button fx:id="statistiksNappi" layoutX="35.0" layoutY="463.0" mnemonicParsing="false" onAction="#statistiksNappiPainettu" opacity="0.0" prefHeight="84.0" prefWidth="382.0" text="Button" />
      <Button fx:id="bankNappi" layoutX="530.0" layoutY="166.0" mnemonicParsing="false" onAction="#bankNappiPainettu" opacity="0.0" prefHeight="84.0" prefWidth="254.0" text="Button" />
      <Button fx:id="cardiilershipNappi" layoutX="527.0" layoutY="268.0" mnemonicParsing="false" onAction="#cardiilershipNappiPainettu" opacity="0.0" prefHeight="84.0" prefWidth="482.0" text="Button" />
      <Button fx:id="manitsmentNappi" layoutX="522.0" layoutY="372.0" mnemonicParsing="false" onAction="#manitsmentNappiPainettu" opacity="0.0" prefHeight="84.0" prefWidth="442.0" text="Button" />
      <Button fx:id="sponsorsNappi" layoutX="523.0" layoutY="468.0" mnemonicParsing="false" onAction="#sponsorsNappiPainettu" opacity="0.0" prefHeight="84.0" prefWidth="368.0" text="Button" />
      <Button fx:id="paluuNappi" layoutX="15.0" layoutY="697.0" mnemonicParsing="false" onAction="#paluuNappiPainettu" opacity="0.0" prefHeight="37.0" prefWidth="52.0" text="Button" />
      <Button fx:id="lopetusNappi" layoutX="964.0" layoutY="703.0" mnemonicParsing="false" onAction="#lopetusNappiPainettu" opacity="0.0" prefHeight="30.0" prefWidth="52.0" text="Button" />
      <Label fx:id="rallinimiLabel" layoutX="575.0" layoutY="44.0" text="RALLINIMI" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label fx:id="ektiedotLabel" layoutX="575.0" layoutY="76.0" text="EK TIEDOT" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label fx:id="sarjatiedotLabel" layoutX="574.0" layoutY="107.0" text="SARJATIEDOT" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label fx:id="car1nimiLabel" layoutX="71.0" layoutY="580.0" text="CAR1NIMI" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label fx:id="car2nimiLabel" layoutX="562.0" layoutY="581.0" text="CAR2NIMI" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label fx:id="kuski2nimilabel" layoutX="562.0" layoutY="610.0" text="KUSKI2NIMI" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label fx:id="performance2label" layoutX="561.0" layoutY="638.0" text="PERFORMANCE2" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label fx:id="performance1label" layoutX="71.0" layoutY="643.0" text="PERFORMANCE1" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
