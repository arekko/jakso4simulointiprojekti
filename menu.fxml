      </Label>
      <Label fx:id="kuski1nimilabel" layoutX="70.0" layoutY="611.0" text="KUSKI1NIMI" textFill="WHITE">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Label fx:id="ready1Label" layoutX="364.0" layoutY="598.0" prefHeight="74.0" prefWidth="103.0" rotate="-38.4" text="READY1" textFill="WHITE">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Label fx:id="ready2Label" layoutX="857.0" layoutY="598.0" prefHeight="84.0" prefWidth="103.0" rotate="-45.0" text="READY2" textFill="WHITE">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
