<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="maxattak.MenuController">
   <children>
      <ImageView fitHeight="768.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Menu.png" />
         </image>
      </ImageView>
      <Button fx:id="startraceNappi" layoutY="32.0" mnemonicParsing="false" onAction="#startraceNappiPainettu" opacity="0.0" prefHeight="114.0" prefWidth="526.0" text="Button" />
