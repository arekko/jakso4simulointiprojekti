            <Button fx:id="kauppaNappi" mnemonicParsing="false" onAction="#kauppaNappiPainettu" prefHeight="325.0" prefWidth="514.0" text="Kauppa" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="sarjataulukkoNappi" mnemonicParsing="false" onAction="#sarjataulukkoNappiPainettu" prefHeight="325.0" prefWidth="514.0" text="Sarjataulukko" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
